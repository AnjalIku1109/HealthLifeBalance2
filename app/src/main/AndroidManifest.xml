<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthassist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.home.Activity3"></activity>
        <activity android:name=".ui.home.ExampleThreeActivity" />
        <activity android:name=".ui.home.ExerciseActivity" />
        <activity android:name=".ui.home.ChoiceActivity" />
        <activity android:name=".ui.CalorieIntake.CalorieActivity" />
        <activity android:name=".ui.CalorieIntake.Calorie2Activity" />
        <activity android:name=".ui.Remainder.RemainderActivity" />
        <activity android:name=".ui.home.MealPlanActivity" />
        <activity android:name=".ui.Prediction.ResultActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.DetailActivity" />
        <activity android:name=".ui.home.ExampleActivity" />
        <activity android:name=".ui.home.exampleOne" />
        <activity android:name=".ui.home.ExampleTwo" />
        <activity android:name=".ui.home.FirstAidActivity" />
        <activity android:name=".ui.home.HomeRemedyActivity" />
        <activity android:name=".ui.home.YogaActivity" />
        <receiver
            android:name=".ui.Remainder.AlarmInitReceiver"
            android:enabled="true">
            <intent-filter>
                <!-- Make sure to update AlarmInitReceiver.kt if you edit this list -->
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.LOCALE_CHANGED" />
                <action android:name="android.intent.action.MY_PACKAGE_REPLACED" />
                <action android:name="android.intent.action.TIME_SET" />
                <action android:name="android.intent.action.TIMEZONE_CHANGED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".ui.Remainder.AlarmReceiver" />
    </application>

</manifest>